        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:Supercluster.VisualTests.Views"
        Title="MainWindow"
        Width="525"
        Height="350"
        mc:Ignorable="d">
    <Grid>

        <TabControl Grid.Column="1" TabStripPlacement="Left">
            <TabItem Header="KnnView">
                <views:KnnView />
            </TabItem>
            <TabItem Header="Data Designer">
                <views:DataDesigner />
            </TabItem>
        </TabControl>

    </Grid>
</Window>
